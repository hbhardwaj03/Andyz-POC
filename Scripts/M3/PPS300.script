end captureremainingquantity

To EnterRecivedQuan RMquanty
	Click "M3/PPS300/Reevedqty"
	TypeText RMquanty
	wait 3
	Click "M3/PPS300/Next"
End EnterRecivedQuan

to enterRecievingNumberinPPS310 ReciveNumber
	"M3/PPS310".open
	if ImageFound(30, "M3/PPS300/PPS310/PPS310A_1") then 
		
		click "M3/PPS300/PPS310/Receivingno"
		TypeText  controlkey, "a"
		TypeText deleteKey
		Typetext ReciveNumber
		//TypeText Return
		Click "M3/PPS300/Next"
		Logsuccess "Reciving Number entered in PPS310 was :- "& ReciveNumber
		wait 4
	else if Imagefound("M3/PPS300/PPS310/PPS310B1") then
		Click "M3/PPS300/Actions"
		Click "M3/PPS300/PPS310/SettingsF13"
		wait 5
		Click "M3/PPS300/PPS310/Openingpanel"
		Typetext upArrow
		Typetext return
		Click "M3/PPS300/Next"
		wait 4
		click "M3/PPS300/PPS310/Receivingno"
		TypeText  controlkey, "a"
		TypeText deleteKey
		Typetext ReciveNumber
		//TypeText Return
		Click "M3/PPS300/Next"
		wait 4
		Logsuccess "Reciving Number entered in PPS310 was :- "& ReciveNumber
	else
Logerror "Reciving Number not entered in PPS310"
		
	End if
	
end enterRecievingNumberinPPS310

to zoomIn x
	KeyDown controlKey
	repeat x times
		TypeText "="
	end repeat
	KeyUp controlKey
end zoomIn

to zoomOut x
	KeyDown controlKey
	repeat x times
		TypeText "-"
	end repeat
	KeyUp controlKey
end zoomOut


















